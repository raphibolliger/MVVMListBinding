<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.View">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane prefHeight="297.0" prefWidth="410.0">
         <children>
            <Button fx:id="submitButton" layoutX="330.0" layoutY="225.0" mnemonicParsing="false" onAction="#btnClicked" text="Add" />
            <TextField fx:id="textInput" layoutX="12.0" layoutY="225.0" prefHeight="26.0" prefWidth="304.0" />
            <ListView fx:id="textList" layoutX="12.0" layoutY="13.0" onKeyPressed="#keyPressedRemove" prefHeight="203.0" prefWidth="382.0" />
            <Button fx:id="removeButton" layoutX="330.0" layoutY="255.0" mnemonicParsing="false" onAction="#btnRemoveClicked" text="Remove" />
         </children>
      </Pane>
   </children>
</GridPane>
